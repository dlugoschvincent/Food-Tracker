{
	"name": "food-tracker",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "NODE_OPTIONS='--inspect-brk=0.0.0.0' vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"prebuild": "prisma generate",
		"predev": "prisma generate && prisma db push --accept-data-loss",
		"prestart": "prisma db push --accept-data-loss",
		"preview": "vite preview",
		"start": "node build"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^2.7.2",
		"@auth/sveltekit": "^1.7.2",
		"@ericblade/quagga2": "^1.8.4",
		"@prisma/client": "^5.21.1"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.265",
		"@playwright/test": "^1.48.2",
		"@sveltejs/adapter-node": "5.2.9",
		"@sveltejs/kit": "^2.7.3",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "^9.6.1",
		"@unocss/extractor-svelte": "^0.63.6",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.11.0",
		"prettier": "^3.3.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-package": "^1.4.0",
		"prettier-plugin-svelte": "^3.2.7",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"prisma": "^5.21.1",
		"svelte": "^5.1.4",
		"svelte-check": "^4.0.5",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.12.2",
		"unocss": "^0.63.6",
		"vite": "5.4.10"
	}
}
