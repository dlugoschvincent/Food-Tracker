{
	"name": "food-tracker",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "NODE_OPTIONS='--inspect-brk=0.0.0.0' vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"prebuild": "prisma generate",
		"predev": "prisma generate && prisma db push --accept-data-loss",
		"prestart": "prisma db push --accept-data-loss",
		"preview": "vite preview",
		"start": "node build"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^2.7.4",
		"@auth/sveltekit": "^1.7.4",
		"@ericblade/quagga2": "^1.8.4",
		"@prisma/client": "^5.22.0"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.279",
		"@playwright/test": "^1.49.0",
		"@sveltejs/adapter-node": "5.2.9",
		"@sveltejs/kit": "^2.9.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.1",
		"@types/eslint": "^9.6.1",
		"@unocss/extractor-svelte": "^0.65.1",
		"eslint": "^9.16.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.13.0",
		"prettier": "^3.4.2",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-package": "^1.4.0",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"prisma": "^5.22.0",
		"svelte": "^5.5.3",
		"svelte-check": "^4.1.1",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.17.0",
		"unocss": "^0.65.1",
		"vite": "^6.0.2"
	}
}
